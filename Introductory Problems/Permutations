void solve(){
int n;cin>>n;
if(n==3 || n==2){cout<<"NO SOLUTION";return;}
if(n==4){
    cout<<"2 4 1 3 ";return;
}
vector<int>ans;
for(int i=1;i<=n;i+=2)ans.push_back(i);
for(int i=2;i<=n;i+=2)ans.push_back(i);
for(auto x:ans)cout<<x<<" ";


}
